    "@mui/x-tree-view": "^6.17.0",
    "apexcharts": "^3.44.2",
    "axios": "^1.6.0",
    "cookie": "^0.6.0",
    "dequal": "^2.0.3",
    "jwt-decode": "^4.0.0",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-apexcharts": "^1.4.1",
    "react-dom": "^18.2.0",
